const dnaToRna = (dna) => {
  if (dna === '') {
    return '';
  }
  let rna = '';
  for (let i = 0; i < dna.length; i += 1) {
    if (dna[i] !== 'A' && dna[i] !== 'C' && dna[i] !== 'G' && dna[i] !== 'T') {
      return null;
    }
    if (dna[i] === 'A') {
      rna += 'U';
    } else if (dna[i] === 'C') {
      rna += 'G';
    } else if (dna[i] === 'G') {
      rna += 'C';
    } else if (dna[i] === 'T') {
      rna += 'A';
    }
  }
  return rna;
};